<div class="spacer"></div>
<div class="col s12 m6 l4">
  <div class="card">
    <div class="card-image">
      <img src="images/<%= item.image_link %>">
      <span class="card-title accent-font frosting-text"><%= item.name %></span>
    </div>
    <div class="card-content text-font vanilla chocolate-text">
      <h5>$<%= item.price %> each</h5>
      <h5>$<%= item.dozen_price %> per dozen</h5>
    </div>
  </div>
</div>